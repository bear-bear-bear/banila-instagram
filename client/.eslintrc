{
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
    ],
  "env": {
    "browser": true,
    "node": true
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "prettier/prettier": ["error", {
        "endOfLine": "auto"
    }],
    "import/no-cycle": "off",
    "no-shadow": "warn"
  }
}

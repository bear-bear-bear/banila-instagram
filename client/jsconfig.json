{
  "compilerOptions": {
      "target": "esnext",
      "allowSyntheticDefaultImports": false,
      "baseUrl": "app",
      "paths": {
        "@/*": ["./*"],
        "@assets/*": ["assets/*"],
        "@fonts/*": ["assets/fonts/*"],
        "@images/*": ["assets/images/*"],
        "@templates/*": ["templates/*"],
        "@pages/*": ["pages/*"],
        "@common/*": ["common/*"],
        "@lib/*": ["lib/*"],
      }
  },
  "include": ["./**/*.*"],
  "exclude": ["node_modules"]
}

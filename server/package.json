{
  "name": "vanilla-instagram-server",
  "version": "1.0.0",
  "description": "Typescript, koa.js",
  "main": "src/server.ts",
  "scripts": {
    "prepare": "cd ../ && npx husky install server/.husky",
    "build": "cross-env NODE_ENV=production swc --config-file .swcrc.json src/ -d dist &",
    "start": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "test": "cross-env NODE_ENV=development jest",
    "lint": "eslint .",
    "db:create": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/lib/createDatabase.ts"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint . --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bear-bear-bear/vanilla-instagram.git"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "koa-session": "^6.2.0",
    "koa-static": "^5.0.0",
    "mariadb": "^2.5.4",
    "mysql2": "^2.3.0",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.6.5",
    "twilio": "^3.67.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@shopify/jest-koa-mocks": "^3.0.4",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.24",
    "@types/koa": "^2.13.4",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa-session": "^5.10.4",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^16.4.3",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^27.0.6",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "husky": "^4.2.3",
    "jest": "^27.0.6",
    "lint-staged": "^10.0.0-1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.5",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "author": "",
  "license": "ISC",
  "bugs": {

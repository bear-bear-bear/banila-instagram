{
  "name": "vanilla-instagram-server",
  "version": "1.0.0",
  "description": "Typescript, koa.js",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "cd ../ && npx husky install server/.husky",
    "test": "jest --detectOpenHandles",
    "build": "cross-env NODE_ENV=production swc --config-file .swcrc.json src/ -d dist &",
    "start": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "lint": "eslint ."
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint . --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bear-bear-bear/vanilla-instagram.git"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "regenerator-runtime": "^0.13.9",
    "snowpack": "^3.8.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@swc/cli": "^0.1.46",
    "@swc/core": "^1.2.66",
    "@types/jest": "^26.0.24",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^16.4.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^27.0.6",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.2.3",
    "jest": "^27.0.6",
    "lint-staged": "^10.0.0-1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },

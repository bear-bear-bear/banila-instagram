{
  "test": ".*.ts$",
  "exclude": ["node_modules$"],
  "jsc": {
    "target": "es2015",
    "loose": true,
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    }
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  }
}
